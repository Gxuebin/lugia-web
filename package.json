{
  "name": "sv-widget",
  "version": "1.0.3",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "2.2.5",
    "css-animation": "1.4.1",
    "prop-types": "15.5.10",
    "prop-types-extra": "1.0.1",
    "rc-align": "2.x",
    "rc-animate": "2.x",
    "rc-util": "4.x",
    "react": "^16.2.0",
    "react-dev-utils": "3.0.2",
    "react-dom": "^16.2.0",
    "react-error-overlay": "1.0.9",
    "style-loader": "0.18.2",
    "styled-components": "2.1.2",
    "warning": "^3.0.0"
  },
  "author": "ysstech",
  "scripts": {
    "lint:css": "stylelint './src/widgets/**/*.js'",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "cover": "node scripts/test.js --env=jsdom  --coverage"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-react-flow-props-to-prop-types": "0.15.0",
    "babel-preset-react-app": "3.1.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "4.1.0",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "debug": "2.6.8",
    "dotenv": "4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "flow-bin": "0.57.3",
    "fs-extra": "3.0.1",
    "gulp": "3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "3",
    "gulp-flowtype": "1",
    "gulp-newer": "1",
    "gulp-rename": "1.2.2",
    "gulp-uglify": "2",
    "gulp-watch": "4",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "3.0.0",
    "immutable": "3.8.1",
    "install": "0.10.1",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "jest-styled-components": "4.2.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "pump": "1.0.2",
    "react-test-renderer": "16.0.0",
    "stylelint": "8.0.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-processor-styled-components": "0.2.1",
    "sv-test-utils": "^0.1.3",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "vx-mock": "0.1.7",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/demo*.js",
      "!src/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/?(*.)(spec|test).js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "react-native$": "react-native-web",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "main": "dist",
  "files": [
    "dist",
    "interface"
  ],
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "react-flow-props-to-prop-types"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
