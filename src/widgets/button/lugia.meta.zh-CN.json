{
  "widgetName": "Button",
  "title": "Button按钮",
  "desc": "方便用户点击操作",
  "props": {
    "disabled": {
      "type": "boolean",
      "desc": "true 禁用 false 可用",
      "defaultValue": true
    },
    "shape": {
      "type": "ButtonShape",
      "desc": "支持多种不同的按钮风格",
      "defaultValue": "default"
    }
  },
  "events": {
    "onClick": {
      "desc": "点击按钮时触发",
      "args": [
        {
          "name": "event",
          "desc": "点击的DOM事件",
          "type": "Object"
        }
      ]
    }
  },
  "type": {
    "ButtonShape": [
      "default",
      "round"
    ],
    "ButtonStyle": {
      "width": {
        "type": "number",
        "desc": "组件宽度",
      }
    }
  },
  "category": ["通用"]
}
